    create table mscan.outpatient2010 as
    select a.enrolid, a.svcdate, a.dx1, a.dx2, a.dx3, a.dx4
    from source.ccaeo103 a
    join mscan.analytical_cohort b
    on a.enrolid = b.enrolid;
quit;

