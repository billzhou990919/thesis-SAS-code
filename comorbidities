        dx2 in: ('250', 'E08', 'E09', 'E10', 'E11', 'E13') or 
        dx3 in: ('250', 'E08', 'E09', 'E10', 'E11', 'E13') or 
        dx4 in: ('250', 'E08', 'E09', 'E10', 'E11', 'E13')) then flag = 1;
    else flag = 0;
    keep enrolid flag;
run;

proc sort data=mscan.flag_diabetes nodupkey;
    by enrolid;
    where flag = 1;
run;

data mscan.analytical_cohort;
    merge mscan.analytical_cohort(in=a) mscan.flag_diabetes(in=b);
    by enrolid;
    if b then diabetes = 1;
    else diabetes = 0;
    drop flag;
run;
